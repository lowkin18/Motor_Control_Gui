<UserControl x:Class="sensors_gui.setup_content"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:sensors_gui"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <ComboBox x:Name="port_box" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="120" />
        <ComboBox x:Name="baudrate_box" HorizontalAlignment="Left" Margin="170,70,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="9600" />
            <ComboBoxItem Content="115200" />
            <ComboBoxItem Content="460800" />
        </ComboBox>
        <Label x:Name="label" Content="Port Number" HorizontalAlignment="Left" Margin="10,39,0,0" VerticalAlignment="Top" Width="78" />
        <Label x:Name="label1" Content="Baudrate" HorizontalAlignment="Left" Margin="170,39,0,0" VerticalAlignment="Top" Width="88" />
    </Grid>
</UserControl>